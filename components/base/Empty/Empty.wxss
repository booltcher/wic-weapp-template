.empty-com {
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
  box-sizing: border-box;
  padding: 32px 0;
  background: #fff;
}
.empty-com__image {
  width: 162px;
  height: 160px;
}
.empty-com__image:empty {
  display: none;
}
.empty-com__image__img {
  width: 100%;
  height: 100%;
}
.empty-com__description {
  margin-top: 16px;
  padding: 0 60px;
  color: #999;
  font-size: 14px;
  line-height: 20px;
}
.empty-com__description:empty {
  display: none;
}
.empty-com__bottom {
  margin-top: 24px;
}
