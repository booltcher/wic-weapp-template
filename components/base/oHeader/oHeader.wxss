.custom-system-header{
    width: 100%;
    position: fixed;
    top: 0;
    background-color: var(--color_header);
    box-sizing: border-box;
    color: #fff;
    z-index: 50;
}
.custom-header-inner{
    width: 100%;
    display: flex;
    align-items: center;
    justify-content: center;
}
.custom-header-before{
    position: absolute;
    left: 10px;
}
