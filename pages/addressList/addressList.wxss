page{
  background-color: #fff;
}
.container {
  padding-top: 20rpx;
  padding-bottom: 130rpx;
  background-color: #fff;
}

.list-wrap {
  padding-top: 30rpx;
  padding-bottom: 30rpx;
}

.fixed-btn-wrap {
  position: fixed;
  bottom: 0;
  display: flex;
  align-items: center;
  justify-content: space-around;
  width: 100%;
  padding: 30rpx 0;
  background-color: #fff;
}

.fixed-btn {
  width: 330rpx;
  height: 70rpx;
  border-radius: 70rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  font-size: 26rpx;
}

.fixed-btn--custom {
  background-color: var(--color-main);
  color: #fff;
}

.fixed-btn--wx {
  background-color: var(--color-wx);
  color: #fff;
}
