      /* --color_header: #ff6381; */
      --color_header: #fe75a9;
      --color_main: #fe75a9;
      --color_header_a: rgba(254, 117, 169, 0.7);
      --color_green2: #ff758f;
      --color_main_deep: #fd649f;
      --color_gray: #f4f4f4;
      --color_font: #313131;
      --color_disabled: #999;
      --color_border: #EBEEF5;
      --color_border_deep: #E4E7ED;
      --color_error: #F56C6C;
      --color_success: #67C23A;
      --color_success_a: rgba(103, 194, 58, 0.7);
      --color_waiting: #ff9a2a;
      --color_waiting_a: rgba(255, 154, 42, 0.7);
      --color_canceled: #909399;
      --color_canceled_a: rgba(144, 147, 153, 0.7);

      font-size: 26rpx;
}

